export FNM_LOGLEVEL="error"
export FNM_COREPACK_ENABLED="true"
export FNM_RESOLVE_ENGINES="true"
export FNM_VERSION_FILE_STRATEGY="recursive"
export GOPATH="$HOME/go"
export GOBIN="$GOPATH/bin"
export PATH="$HOME/bin:$HOME:$HOME/.cargo/bin:$GOBIN:$PATH"
export TERMINAL="zellij run -c -- "
export EDITOR="helix"
export COREPACK_ENABLE_AUTO_PIN="0"
export COREPACK_ENABLE_DOWNLOAD_PROMPT="0"
alias ls="lsd"
alias hx="helix"
alias pi="yarn"
alias pe="yarn run -T -B"
alias pem="yarn run -T -B monochromatic"
alias pemb="yarn run -T -B monochromatic build"
alias pems="yarn run -T -B monochromatic serve"
alias pemc="yarn run -T -B monochromatic clean"
alias pemw="yarn run -T -B monochromatic watch"
